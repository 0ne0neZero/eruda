<ul>
    {{#each logs}}
        <li class="eruda-{{type}}">
            {{#if showTimes}}<span class="eruda-times">{{times}}</span>{{/if}}
            {{#if isCode}}
                <pre><code>{{{val}}}</code></pre>
            {{else}}
                {{{val}}}
            {{/if}}
        </li>
    {{/each}}
</ul>